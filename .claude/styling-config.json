{
  "themeName": "stackone-neumorphic",
  "source": "https://www.stackone.com/",
  "style": "neumorphism",
  "tokens": {
    "colors": {
      "primary": "#00af66",
      "primary-foreground": "#FFFFFF",
      "primary-hover": "#008a52",
      "secondary": "#4d4eba",
      "secondary-foreground": "#FFFFFF",
      "secondary-hover": "#4338ca",
      "background": "#f0f0f0",
      "foreground": "#222222",
      "surface": "#f0f0f0",
      "muted": "#e4e4e4",
      "muted-foreground": "#6b7280",
      "border": "#d8d8d8",
      "destructive": "#ef3737",
      "destructive-foreground": "#FFFFFF",
      "destructive-hover": "#b91c1c",
      "accent": "#6366f1",
      "accent-foreground": "#FFFFFF",
      "success": "#00af66",
      "success-foreground": "#FFFFFF",
      "warning": "#ff6900",
      "warning-foreground": "#FFFFFF",
      "info": "#3b82f6",
      "info-foreground": "#FFFFFF",
      "spinner-front": "#00af66",
      "spinner-back": "#00935a",
      "spinner-light": "#d8d8d8",
      "spinner-lighter": "#e6e6e6",
      "spinner-dark": "#404040",
      "spinner-darker": "#525252"
    },
    "neumorphic": {
      "base": "#f0f0f0",
      "light": "rgba(255, 255, 255, 0.88)",
      "dark": "rgba(185, 185, 185, 0.48)",
      "lightIntense": "rgba(255, 255, 255, 1)",
      "darkIntense": "rgba(170, 170, 170, 0.58)"
    },
    "radii": {
      "none": "0",
      "sm": "0.375rem",
      "md": "0.625rem",
      "lg": "1rem",
      "xl": "1.5rem",
      "2xl": "2rem",
      "full": "9999px",
      "default": "lg"
    },
    "shadows": {
      "none": "none",
      "raised": "-6px -6px 14px rgba(255, 255, 255, 0.88), 6px 6px 14px rgba(185, 185, 185, 0.48)",
      "raised-sm": "-3px -3px 8px rgba(255, 255, 255, 0.88), 3px 3px 8px rgba(185, 185, 185, 0.45)",
      "raised-lg": "-8px -8px 20px rgba(255, 255, 255, 0.9), 8px 8px 20px rgba(185, 185, 185, 0.55)",
      "pressed": "inset -4px -4px 8px rgba(255, 255, 255, 0.88), inset 4px 4px 8px rgba(185, 185, 185, 0.45)",
      "pressed-sm": "inset -2px -2px 5px rgba(255, 255, 255, 0.75), inset 2px 2px 5px rgba(185, 185, 185, 0.4)",
      "flat": "inset 1px 1px 2px rgba(185, 185, 185, 0.28), inset -1px -1px 2px rgba(255, 255, 255, 0.55)",
      "focus": "0 0 0 3px rgba(0, 175, 102, 0.6)",
      "inset-highlight": "inset 0 1px 0 rgba(255, 255, 255, 0.2)",
      "variant-primary": "inset 0 1px 0 rgba(255, 255, 255, 0.2), -4px -4px 10px rgba(255, 255, 255, 0.3), 4px 4px 10px rgba(0, 100, 60, 0.3)",
      "variant-secondary": "inset 0 1px 0 rgba(255, 255, 255, 0.2), -4px -4px 10px rgba(255, 255, 255, 0.3), 4px 4px 10px rgba(60, 60, 140, 0.3)",
      "variant-destructive": "inset 0 1px 0 rgba(255, 255, 255, 0.2), -4px -4px 10px rgba(255, 255, 255, 0.3), 4px 4px 10px rgba(180, 40, 40, 0.3)",
      "variant-success": "inset 0 1px 0 rgba(255, 255, 255, 0.2), -4px -4px 10px rgba(255, 255, 255, 0.3), 4px 4px 10px rgba(0, 100, 60, 0.2)",
      "variant-warning": "inset 0 1px 0 rgba(255, 255, 255, 0.2), -4px -4px 10px rgba(255, 255, 255, 0.3), 4px 4px 10px rgba(180, 80, 0, 0.2)",
      "control-unchecked": "inset -2px -2px 4px rgba(255, 255, 255, 0.4), inset 2px 2px 4px rgba(0, 60, 40, 0.25)",
      "control-checked": "inset 0 1px 0 rgba(255, 255, 255, 0.2), inset 2px 2px 4px rgba(0, 80, 50, 0.3)"
    },
    "spacing": {
      "tight": "1",
      "normal": "1.25",
      "loose": "2.5"
    },
    "typography": {
      "families": {
        "heading": {
          "name": "Inter",
          "stack": "'Inter', ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
          "variable": "--font-heading"
        },
        "body": {
          "name": "Inter",
          "stack": "'Inter', ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
          "variable": "--font-body"
        },
        "code": {
          "name": "IBM Plex Mono",
          "stack": "'IBM Plex Mono', ui-monospace, SFMono-Regular, 'SF Mono', Menlo, Consolas, monospace",
          "variable": "--font-code"
        }
      },
      "scale": {
        "xs": {
          "size": "0.75rem",
          "lineHeight": "1.2",
          "letterSpacing": "0"
        },
        "sm": {
          "size": "0.875rem",
          "lineHeight": "1.25",
          "letterSpacing": "0"
        },
        "base": {
          "size": "1rem",
          "lineHeight": "1.6",
          "letterSpacing": "0"
        },
        "lg": {
          "size": "1.125rem",
          "lineHeight": "1.6",
          "letterSpacing": "-0.01em"
        },
        "xl": {
          "size": "1.25rem",
          "lineHeight": "1.5",
          "letterSpacing": "-0.01em"
        },
        "2xl": {
          "size": "1.5rem",
          "lineHeight": "1.25",
          "letterSpacing": "-0.015em"
        },
        "3xl": {
          "size": "1.875rem",
          "lineHeight": "1.2",
          "letterSpacing": "-0.02em"
        },
        "4xl": {
          "size": "2.25rem",
          "lineHeight": "1.2",
          "letterSpacing": "-0.025em"
        }
      },
      "weights": {
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700"
      },
      "googleFonts": {
        "url": "https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=IBM+Plex+Mono:wght@400;500&display=swap",
        "preconnect": true
      }
    }
  },
  "motion": {
    "transition": {
      "fast": "0.15s ease-out",
      "normal": "0.2s ease-out",
      "slow": "0.3s ease-out"
    },
    "press": {
      "scale": 0.98,
      "duration": 0.1
    },
    "spring": {
      "default": {
        "stiffness": 300,
        "damping": 30
      },
      "bouncy": {
        "stiffness": 400,
        "damping": 30
      },
      "gentle": {
        "stiffness": 200,
        "damping": 25
      },
      "snappy": {
        "stiffness": 500,
        "damping": 30
      },
      "tooltip": {
        "stiffness": 400,
        "damping": 15,
        "mass": 0.8
      }
    },
    "duration": {
      "instant": 0.1,
      "fast": 0.15,
      "normal": 0.2,
      "slow": 0.3,
      "spinner": 3,
      "progress": 1.2,
      "loading": 0.2,
      "scroll": 1.2
    },
    "offset": {
      "popover": 4,
      "dropdown": 4,
      "tooltip": 4,
      "slide": 8,
      "icon": 16,
      "knob": 24
    },
    "opacity": {
      "backdrop": 0.8,
      "drawerBackdrop": 0.6,
      "highlight": 0.2
    },
    "lerp": {
      "scroll": 0.1
    },
    "multiplier": {
      "wheel": 1,
      "touch": 2
    }
  },
  "text": {
    "aria": {
      "close": "Close",
      "loading": "Loading...",
      "previousSlide": "Previous slide",
      "nextSlide": "Next slide",
      "goToSlide": "Go to slide",
      "morePages": "More pages",
      "previousPage": "Go to previous page",
      "nextPage": "Go to next page",
      "decreaseValue": "Decrease value",
      "increaseValue": "Increase value",
      "breadcrumb": "breadcrumb",
      "switchToLight": "Switch to light mode",
      "switchToDark": "Switch to dark mode"
    },
    "labels": {
      "previous": "Previous",
      "next": "Next",
      "loading": "LOADING",
      "searchPlaceholder": "Search...",
      "selectPlaceholder": "Select..."
    }
  }
}