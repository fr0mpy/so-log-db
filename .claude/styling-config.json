{
  "themeName": "stackone-neumorphic",
  "source": "https://www.stackone.com/",
  "style": "neumorphism",
  "tokens": {
    "colors": {
      "primary": "#00af66",
      "primary-foreground": "#FFFFFF",
      "secondary": "#6366f1",
      "secondary-foreground": "#FFFFFF",
      "background": "#e4e8eb",
      "foreground": "#2b2b2b",
      "surface": "#e4e8eb",
      "muted": "#d1d5d8",
      "muted-foreground": "#6b7280",
      "border": "#c5cace",
      "destructive": "#ef3737",
      "destructive-foreground": "#FFFFFF",
      "accent": "#6366f1",
      "accent-foreground": "#FFFFFF",
      "success": "#00af66",
      "success-foreground": "#FFFFFF",
      "warning": "#ff6900",
      "warning-foreground": "#FFFFFF"
    },
    "neumorphic": {
      "base": "#e4e8eb",
      "light": "rgba(255, 255, 255, 0.8)",
      "dark": "rgba(163, 177, 198, 0.6)",
      "lightIntense": "rgba(255, 255, 255, 1)",
      "darkIntense": "rgba(136, 150, 168, 0.7)"
    },
    "radii": {
      "none": "0",
      "sm": "0.375rem",
      "md": "0.625rem",
      "lg": "1rem",
      "xl": "1.5rem",
      "2xl": "2rem",
      "full": "9999px",
      "default": "lg"
    },
    "shadows": {
      "none": "none",
      "raised": "-6px -6px 14px rgba(255, 255, 255, 0.8), 6px 6px 14px rgba(163, 177, 198, 0.6)",
      "raised-sm": "-3px -3px 8px rgba(255, 255, 255, 0.8), 3px 3px 8px rgba(163, 177, 198, 0.5)",
      "raised-lg": "-8px -8px 20px rgba(255, 255, 255, 0.9), 8px 8px 20px rgba(163, 177, 198, 0.7)",
      "pressed": "inset -4px -4px 8px rgba(255, 255, 255, 0.8), inset 4px 4px 8px rgba(163, 177, 198, 0.5)",
      "pressed-sm": "inset -2px -2px 5px rgba(255, 255, 255, 0.7), inset 2px 2px 5px rgba(163, 177, 198, 0.4)",
      "flat": "inset 1px 1px 2px rgba(163, 177, 198, 0.3), inset -1px -1px 2px rgba(255, 255, 255, 0.5)",
      "focus": "0 0 0 3px rgba(0, 175, 102, 0.25)"
    },
    "spacing": {
      "tight": "1",
      "normal": "1.25",
      "loose": "2.5"
    },
    "typography": {
      "families": {
        "heading": {
          "name": "Inter",
          "stack": "'Inter', ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
          "variable": "--font-heading"
        },
        "body": {
          "name": "Inter",
          "stack": "'Inter', ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
          "variable": "--font-body"
        },
        "code": {
          "name": "IBM Plex Mono",
          "stack": "'IBM Plex Mono', ui-monospace, SFMono-Regular, 'SF Mono', Menlo, Consolas, monospace",
          "variable": "--font-code"
        }
      },
      "scale": {
        "xs": { "size": "0.75rem", "lineHeight": "1.2", "letterSpacing": "0" },
        "sm": { "size": "0.875rem", "lineHeight": "1.25", "letterSpacing": "0" },
        "base": { "size": "1rem", "lineHeight": "1.6", "letterSpacing": "0" },
        "lg": { "size": "1.125rem", "lineHeight": "1.6", "letterSpacing": "-0.01em" },
        "xl": { "size": "1.25rem", "lineHeight": "1.5", "letterSpacing": "-0.01em" },
        "2xl": { "size": "1.5rem", "lineHeight": "1.25", "letterSpacing": "-0.015em" },
        "3xl": { "size": "1.875rem", "lineHeight": "1.2", "letterSpacing": "-0.02em" },
        "4xl": { "size": "2.25rem", "lineHeight": "1.2", "letterSpacing": "-0.025em" }
      },
      "weights": {
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700"
      },
      "googleFonts": {
        "url": "https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=IBM+Plex+Mono:wght@400;500&display=swap",
        "preconnect": true
      }
    }
  },
  "motion": {
    "transition": {
      "fast": "0.15s ease-out",
      "normal": "0.2s ease-out",
      "slow": "0.3s ease-out"
    },
    "press": {
      "scale": "0.98",
      "duration": "0.1s"
    }
  }
}
