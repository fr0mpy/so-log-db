# Agent Toolkit MFE Environment Variables
# Copy to .env.local and set values for local development
# Set these in Vercel dashboard for production

# Shell URL for cross-zone navigation (REQUIRED in production)
# Vercel: https://stackone-shell.vercel.app
NEXT_PUBLIC_SHELL_URL=http://localhost:3000

# Base URL for metadata/SEO (REQUIRED in production)
# Vercel: https://stackone-shell.vercel.app/agent-toolkit
NEXT_PUBLIC_BASE_URL=http://localhost:3001/agent-toolkit

# PowerSync URL (optional - feature disabled if not set)
NEXT_PUBLIC_POWERSYNC_URL=
